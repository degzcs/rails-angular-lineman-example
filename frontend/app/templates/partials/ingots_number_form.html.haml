%md-dialog
  %md-content.sticky-container
    .dialog-ingots-form
      %form{:name => "IngotsNumberForm", "ng-submit"=>"IngotsNumberForm.selected_ingots_number.$error.required ? '' : submit_ingots_number()"}
        %h3 Seleccione la cantidad de lingotes
      
        %md-select{"ng-model" => "ingots_number", :placeholder => "Seleccione una cantidad","required"=>"", "name"=>"ingots_number"}
          %md-option{:value => "1"} 1
          %md-option{:value => "2"} 2
          %md-option{:value => "3"} 3
          %md-option{:value => "4"} 4
          %md-option{:value => "5"} 5
          %md-option{:value => "6"} 6
          %md-option{:value => "7"} 7
          %md-option{:value => "8"} 8
          %md-option{:value => "9"} 9
          %md-option{:value => "10"} 10
        %md-button.md-raised{type:"submit"} Ok