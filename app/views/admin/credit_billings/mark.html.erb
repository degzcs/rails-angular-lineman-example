<%= form_for @credit_billing, url: admin_credit_billing_path do |f| %>
  <h2>Ingrese la fecha en la que el usuario pago esta factura</h2>
  <%= f.date_field :payment_date , required: "" %>
  <%= f.hidden_field :payment_flag, :value => true%>
  <br>
  <br>
  <%= link_to "Cancelar", admin_credit_billings_path %>
  <%= f.submit "Marcar como pagado" , data: {confirm: "Esta seguro?"}%>
<% end %>